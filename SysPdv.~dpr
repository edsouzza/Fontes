begin
  {//Abre a Splash
  F_Splash:= TF_Splash.Create(Application);
  F_Splash.Show;
  F_Splash.Update;
  Sleep(5000); //Tempo de execução   30  = 1000   }

  Application.Initialize;
  Application.Title := 'SysPdv ';
  Application.CreateForm(Tdm_Conexao, dm_Conexao);
  Application.CreateForm(TDM_Pesq, DM_Pesq);
  Application.CreateForm(Tdm_DadosSegundo, dm_DadosSegundo);
  Application.CreateForm(TDM_PESQDADOS, DM_PESQDADOS);
